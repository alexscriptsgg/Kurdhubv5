-- Modern Key System GUI (Executor-Ready)

local sg = game.Players.LocalPlayer:WaitForChild("PlayerGui")
local s = game:GetService("StarterGui")

-- Notification helper
local function n(t,x,d)
    s:SetCore("SendNotification",{Title=t,Text=x,Duration=d or 3})
end

-- Settings (updated)
local k = ("1511")
local l = ("https://loot-link.com/s?7QWZdGFL")
local u = ("https://raw.githubusercontent.com/Ninja10908/S4/refs/heads/main/Kurdhub")

-- GUI Setup
local g = Instance.new("ScreenGui")
g.Name = "KS"
g.ResetOnSpawn = false
g.Parent = sg

local f = Instance.new("Frame")
f.Size = UDim2.new(0,300,0,150)
f.Position = UDim2.new(0.5,-150,0.5,-75)
f.BackgroundColor3 = Color3.fromRGB(25,25,25)
f.BorderSizePixel = 0
f.Parent = g

local tb = Instance.new("TextBox")
tb.Size = UDim2.new(0.8,0,0,35)
tb.Position = UDim2.new(0.1,0,0.15,0)
tb.PlaceholderText = "üîë Enter Key..."
tb.BackgroundColor3 = Color3.fromRGB(45,45,45)
tb.TextColor3 = Color3.fromRGB(255,255,255)
tb.TextScaled = true
tb.Parent = f

local sb = Instance.new("TextButton")
sb.Size = UDim2.new(0.35,0,0,35)
sb.Position = UDim2.new(0.1,0,0.55,0)
sb.Text = "‚úÖ Submit"
sb.BackgroundColor3 = Color3.fromRGB(0,200,0)
sb.TextColor3 = Color3.fromRGB(255,255,255)
sb.TextScaled = true
sb.Parent = f

local gb = Instance.new("TextButton")
gb.Size = UDim2.new(0.35,0,0,35)
gb.Position = UDim2.new(0.55,0,0.55,0)
gb.Text = "üîó Get Key"
gb.BackgroundColor3 = Color3.fromRGB(0,120,255)
gb.TextColor3 = Color3.fromRGB(255,255,255)
gb.TextScaled = true
gb.Parent = f

-- Submit button
sb.MouseButton1Click:Connect(function()
    if tb.Text == k then
        n("Success","Correct key! Access granted ‚úÖ",4)
        task.wait(0.5)
        g:Destroy()
        loadstring(game:HttpGet(u))()
    else
        n("Error","‚ùå Wrong key!",4)
    end
end)

-- Get Key button
gb.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(l)
        n("Copied","üîó Key link copied!",4)
    else
        n("Manual Copy","Open: "..l,5)
    end
end)
